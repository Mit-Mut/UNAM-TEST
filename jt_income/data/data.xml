    </record>

    <record id="income_reference_DGAE" model="account.journal">
        <field name="name">Reference DGAE</field>
        <field name="code">DGAE</field>
        <field name="currency_id" ref="base.MXN"/>
        <field name="type">sale</field>
    </record>

    <record id="income_trades_DGOAE" model="account.journal">
        <field name="name">Trades DGOAE</field>
        <field name="code">DGOAE</field>
        <field name="currency_id" ref="base.MXN"/>
        <field name="type">sale</field>
    </record>
    
