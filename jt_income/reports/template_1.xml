            <div class="header" style="border:1px solid #D3D3D3;height:200px;">
                <div class="row">
                    <div class="col-3">
                        <img t-attf-src="data:image/png;base64,{{res_company.header_logo}}" style="width:200px;height:170px;"/>
                    </div>
                    <div class="col-6">
                    	<h4>
                    		PATRONATO UNIVERSITARIO<br/>
                    		TESORERIA
                    	</h4>
                    </div>
                    <div class="col-3">
                        <img t-attf-src="data:image/png;base64,{{res_company.header_logo2}}" style="width:200px;height:170px;"/>
                    </div>
                    
                </div>
            </div>
             <!-- Footer -->

            <div class="footer">
            	<span t-set="sender_template" t-value="o.get_all_employee('application_form_20')"/>
				<table style="width:100%">
                	<tr>
                        <td style="text-align:left;">
							<b><span t-field="sender_template.sender_title"/>&#160;<span t-if="sender_template.sender_emp_id" t-field="sender_template.sender_emp_id.name"/>
                				<br/><span t-field="sender_template.sender_professional_title"/></b>                    
                        </td>
                     </tr>
                </table>
                <br/>
            	<!-- 
                <p style="font-size:15px;text-align:left;"><b><span t-field="sender_template.sender_title"/>&#160;<span t-if="sender_template.sender_emp_id" t-field="sender_template.sender_emp_id.name"/>
                <br/><span t-field="sender_template.sender_professional_title"/></b></p><br/>
                 -->
				    
                <p style="font-size:14px;text-align:left;">
                	<b>C.c.p.&#160;&#160;&#160;&#160; </b>
                	<t t-set="count" t-value="0"/>
            		<t t-foreach="sender_template.employee_ids" t-as="emp">
            				<t t-if="count==1">
            					&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;
            				</t>
            				<t t-set="count" t-value="1"/>
            				<b><span t-field="emp.emp_title"></span>
            					<span t-field="emp.name"/>
            				</b>
            				<span t-field="emp.emp_job_title"/><br/>
            		</t>	
            	</p>	
            </div>
            
        </template>

