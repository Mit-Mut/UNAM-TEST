<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Folio -->
        <record id="seq_folio" model="ir.sequence">
            <field name="name">Folio</field>
            <field name="code">payment.folio</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_batch_sheet_folio" model="ir.sequence">
            <field name="name">Batch Sheet Folio</field>
            <field name="code">batch.sheet.folio</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_bank_transfer_request" model="ir.sequence">
            <field name="name">Bank Transfer Request</field>
            <field name="code">bank.transfer.request</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
