<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_req_open_balance_tree" model="ir.ui.view">
            <field name="name">Opening Balance List view</field>
            <field name="model">request.open.balance</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="operation_number"/>
                    <field name="request_date"/>
                    <field name="user_id"/>
                     <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="view_req_open_balance_form" model="ir.ui.view">
            <field name="name">Opening Form view</field>
            <field name="model">request.open.balance</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <button name="request" type="object" string="Request" class="oe_highlight"
                                 attrs="{'invisible': [('state', '!=', 'draft')]}"/>
                        <field name="state" widget="statusbar"/>
                    </header>
                    <sheet>
                        <field name="create_payment_request" invisible="1"/>
                        <div class="oe_button_box" name="button_box">
                            <button name="action_create_payment_req" type="object" class="oe_stat_button"
                                    widget="statinfo" string="Payment Requests"
                                    icon="fa-random" attrs="{'invisible': [('create_payment_request', '=', False)]}">
<!--&lt;!&ndash;                                <field name="total_operations" widget="statinfo"/>&ndash;&gt;-->
                            </button>
                        </div>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="operation_number" required="1"
                                       attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="agreement_number" readonly="1"/>
                                <field name="type_of_operation"
                                       attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                            </group>
                            <group>
                                <field name="apply_to_basis_collaboration" readonly="1"/>
                                <field name="origin_resource_id" domain="[('is_it_enabled_for_agreement', '=',  True)]"
                                attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="beneficiary_id"/>
                                <field name="bases_collaboration_id" invisible="1"/>
                                <field name="provider_id" context="{'from_retier_operation':1,
                                 'collaboration_id': bases_collaboration_id}"/>
                            </group>
                        </group>
                        <group>
                            <field name="reason_rejection" attrs="{'invisible': [('state', '!=', 'rejected')]}"
                             readonly="1"/>
                        </group>
                        <notebook>
                            <page string="Operation" name="operation">
                                <group>
                                    <group>
                                        <field name="request_date"/>
                                        <field name="trade_number"/>
                                        <field name="opening_balance"/>
                                    </group>
                                    <group>
                                        <field name="observations"/>
                                        <field name="user_id" readonly="1"/>
                                    </group>
                                </group>
                            </page>
                             <page string="Supporting Documentaion" name="supporting_documentation">
                                <group attrs="{'invisible': [('type_of_operation', 'in', ('withdrawal', 'increase',
                                'retirement','withdrawal_cancellation', 'increase_by_closing', 'withdrawal_closure'))]}">
                                    <group>
                                        <field name="cbc_format"/>
                                    </group>
                                    <group>
                                        <field name="cbc_shipping_office"/>
                                    </group>
                                </group>
                                 <group attrs="{'invisible': [('type_of_operation', 'not in', ('withdrawal', 'increase',
                                 'retirement', 'withdrawal_cancellation', 'withdrawal_closure', 'increase_by_closing'))]}">
                                    <group>
                                        <field name="supporting_documentation"/>
                                    </group>
                                </group>
                            </page>
                             <page string="Accounts" name="accounts">
                                <group>
                                    <group>
                                        <field name="liability_account_id"/>
                                        <field name="investment_account_id"/>
                                    </group>
                                    <group>
                                        <field name="interest_account_id"/>
                                        <field name="availability_account_id"/>
                                    </group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_req_open_balance" model="ir.actions.act_window">
            <field name="name">Request Opening Balance</field>
            <field name="res_model">request.open.balance</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('name', '=', context.get('name'))]</field>
        </record>

