<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="account_journal_supplier_layout_inherit_view" model="ir.ui.view">
            <field name="name">account.journal.supplier.layout</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//page[@name='advanced_settings']/group[1]" position="inside">
            		<group name="bank_layout" string="Layout">
            			<field name="supplier_registration_layout"/>
            		</group>
            	</xpath>
			</field>
		</record>		
	</data>
</odoo>