        <record id="view_bases_collabration_tree" model="ir.ui.view">
            <field name="name">Bases Collabration List view</field>
            <field name="model">bases.collaboration</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_bases_collabration_form" model="ir.ui.view">
            <field name="name">Bases Collabration Form view</field>
            <field name="model">bases.collaboration</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <field name="state" widget="statusbar"/>
                        <button name="confirm" type="object" string="Confirm" class="oe_highlight"
                                 attrs="{'invisible': [('state', '!=', 'draft')]}"/>
                        <button name="cancel" type="object" string="Cancel" class="oe_highlight"
                                 attrs="{'invisible': [('state', '!=', 'valid')]}"/>
                    </header>
                    <sheet>
                         <div class="oe_button_box" name="button_box">
                            <button name="action_operations" type="object" class="oe_stat_button"
                                    icon="fa-random" attrs="{'invisible': [('state', '=', 'draft')]}">
                                <field name="total_operations" widget="statinfo"/>
                            </button>
                             <button name="action_modifications" type="object" class="oe_stat_button"
                                    icon="fa-random" attrs="{'invisible': [('state', '=', 'draft')]}">
                                <field name="total_modifications" widget="statinfo"/>
                            </button>

                        </div>
                        <group>
                            <group>
                                <field name="name" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="convention_no" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="opening_bal" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="available_bal" readonly="1"/>
                                <field name="agreement_type_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="fund_type_id" readonly="1" force_save="1"/>
                                <field name="dependency_obs" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                            </group>
                            <group>
                            
                            	<field name="is_specific" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="dependency_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="desc_dependency" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="subdependency_id" attrs="{'readonly': [('state', '!=', 'draft')]}"
                                       domain="[('dependency_id', '=', dependency_id)]"/>
                                <field name="desc_subdependency" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="origin_resource_id" attrs="{'readonly': [('state', '!=', 'draft')]}"
                                       domain="[('is_it_enabled_for_agreement', '=',  True)]"/>
                                <field name="goals" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="registration_date" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Accounts" name="accounts">
                                <group>
                                    <group>
                                        <field name="liability_account_id"
                                        attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="investment_account_id"
                                        attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                    <group>
                                        <field name="interest_account_id"
                                        attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="availability_account_id"
                                        attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Collaboration Base Contacts" name="collaboration_base_contacts">
                                <group>
                                    <group string="Owner of Dependency">
                                        <field name="employee_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="job_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="phone" attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                    <group string="Administrative Secretary">
                                        <field name="administrative_secretary_id"
                                               attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="administrative_secretary_phone"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                    <group string="Responsible Directer of CBC">
                                        <field name="direct_manager_cbc_id"
                                               attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="cbc_responsible_phone"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="email"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                    <group string="Address of Dependency">
                                        <field name="unit_address"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                        <field name="additional_observation"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Registration Documentation" name="registration_documentation">
                                <group>
                                    <group>
                                        <field name="cbc_format" required="1"
                                         attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                    <group>
                                        <field name="cbc_shipping_office" required="1"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Committee" name="committee">
                                <group string="Committe">
                                    <field name="committe_ids" attrs="{'readonly': [('state', '!=', 'draft')]}">
                                        <tree editable="top">
                                            <field name="column_id"/>
                                            <field name="column_position_id"/>
                                        </tree>
                                        <form>
                                            <group>
                                                <field name="column_id"/>
                                                <field name="column_position_id"/>
                                            </group>
                                        </form>
                                    </field>
                                </group>
                            </page>
                             <page string="Beneficiaries Management" name="beneficiaries">
                                <group string="Beneficiaries Sectional">
                                    <group>
                                        <field name="no_beneficiary_allowed"
                                        attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                    </group>
                                    <group>
                                        <field name="beneficiary_ids" nolabel="1"
                                               attrs="{'readonly': [('state', '!=', 'draft')]}">
                                            <tree>
                                                <field name="employee_id" required="1"/>
                                                <field name="bank_id" context="{'from_agreement':1}"
                                                  domain="[('employee_id', '=', employee_id)]"/>
                                                <field name="account_number"/>
                                                <field name="amount"/>
                                                <field name="validity_start"/>
                                                <field name="validity_final_beneficiary"/>
                                                <field name="withdrawal_sch_date"/>
                                            </tree>
                                            <form>
                                                <group>
                                                    <group>
                                                        <field name="employee_id" required="1"/>
                                                        <field name="bank_id"
                                                               domain="[('employee_id', '=', employee_id)]"
                                                               context="{'from_agreement':1}"/>
                                                        <field name="account_number"/>
                                                        <field name="payment_rule_id"/>
                                                    </group>
                                                     <group>
                                                         <field name="amount"/>
                                                        <field name="validity_start"/>
                                                        <field name="validity_final_beneficiary"/>
                                                        <field name="withdrawal_sch_date"/>
                                                    </group>
                                                </group>
                                            </form>
                                        </field>
                                    </group>
                                </group>
                                 <group string="Providers" name="providers">
                                     <field name="provider_ids" nolabel="1"
                                            attrs="{'readonly': [('state', '!=', 'draft')]}">
                                         <tree editable="top">
                                             <field name="partner_id" required="1"/>
                                                <field name="bank_id" domain="[('partner_id', '=', partner_id)]"
                                                context="{'from_agreement':1}"/>
                                                <field name="account_number"/>
                                         </tree>
                                         <form>
                                             <group>
                                                 <field name="partner_id" required="1"/>
                                                <field name="bank_id"
                                                 domain="[('partner_id', '=', partner_id)]"
                                                context="{'from_agreement':1}"/>
                                                <field name="account_number"/>
                                             </group>
                                         </form>
                                     </field>
                                 </group>
                            </page>
                            <page name="cancellation_info" string="Cancellation Information"
                                attrs="{'invisible': [('state', 'not in', ('in_force', 'to_be_cancelled', 'cancelled'))]}">
                                <group>
                                    <field name="cancel_date"/>
                                    <field name="supporing_doc"/>
                                    <field name="reason_cancel"/>
                                </group>
                            </page>
                            <page name="closing_info" string="Information for Closing" attrs="{'invisible':[('is_specific','=',True)]}">
                                <group>
                                    <field name="fund_name_transfer_id"/>
                                    <field name="closing_amt"/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

