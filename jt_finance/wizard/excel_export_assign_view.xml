<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="excel_export_assign_report_form_view" model="ir.ui.view">
            <field name="name">excel.export.assign.report.form.view</field>
            <field name="model">excel.export.assign.report</field>
            <field name="arch" type="xml">
                <form>
                    <group >
                        <field name="filename" invisible="1"/>
                        <field name="fec_data" filename="filename" readonly="1"/>
                    </group>
                    <footer>
                        <button string="Close" class="btn btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>