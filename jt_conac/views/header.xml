<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Header Template of STATEMENT OF FINANCIAL POSITION -->
        <template id="external_layout_of_stment_of_finan_posi" name="external_layout_of_stment_of_finan_posi">
        <t t-if="not o" t-set="o" t-value="doc"/>

        <t t-if="not company">
            <!-- Multicompany -->
            <t t-if="company_id">
                <t t-set="company" t-value="company_id"/>
            </t>
            <t t-elif="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id.sudo()"/>
            </t>
            <t t-else="else">
                <t t-set="company" t-value="res_company"/>
            </t>
        </t>

        <div class="header">
            <div class="row">
                <div class="col-3">
                    <img t-attf-src="data:image/png;base64,{{company.header_logo}}" style="width:120px;height:100px;margin-top:20px;margin-right:20px;margin-bottom:20px;"/>
                </div>
            
                <div class="col-6 text-center">
                    <span style="font-size:16px;">NATIONAL AUTONOMOUS UNIVERSITY OF MEXICO</span><br/>
                    <span>STATEMENT OF FINANCIAL POSITION </span><br/>
                    <span> <t t-esc="header_date"/> </span>
                </div>                
            </div>
        </div>
        <div class="article" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
          <t t-raw="0"/>
        </div>
        </template>

