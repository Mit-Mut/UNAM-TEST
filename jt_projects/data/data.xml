<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sequence_account_request" model="ir.sequence">
        <field name="name">Account Request</field>
        <field name="code">request.accounts</field>
        <field name="active">TRUE</field>
        <field name="padding">2</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="sequence_transfer_request" model="ir.sequence">
        <field name="name">Transfer request project</field>
        <field name="code">request.transfer</field>
        <field name="active">TRUE</field>
        <field name="padding">2</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    
</odoo>